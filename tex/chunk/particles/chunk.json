{
	"format_version":"1.10.0",
	"particle_effect":{
		"description":{
			"identifier":"toko:chunk",
			"basic_render_parameters":{"material":"particles_alpha","texture":"textures/particle/particles"}
		},
		"components":{
			"minecraft:emitter_initialization":{"per_update_expression":"v.t=q.time_stamp/20*8;v.x=math.clamp(math.abs(math.mod(v.t,8)-4)-2,-1,1)*8;v.y=math.clamp(math.abs(math.mod(v.t+2,8)-4)-2,-1,1)*8;v.t=v.particle_random_2*2-1;v.dx=(math.abs(v.x)<8)*v.t;v.dy=(math.abs(v.y)<8)*v.t;v.x=v.x-8+math.mod(math.mod(q.position(0),16)+16,16);v.y=v.y-8+math.mod(math.mod(q.position(2),16)+16,16);v.s=math.sin(-q.body_y_rotation);v.c=math.cos(-q.body_y_rotation);"},
			"minecraft:emitter_rate_steady":{"spawn_rate":64,"max_particles":256},
			"minecraft:emitter_lifetime_looping":{"active_time":2},
			"minecraft:emitter_shape_point":{"offset":["v.x*v.c-v.y*v.s",1,"v.x*v.s+v.y*v.c"],"direction":["v.dx*v.c-v.dy*v.s","math.sqrt(1-v.t*v.t)","v.dx*v.s+v.dy*v.c"]},

			"minecraft:particle_motion_dynamic":{"linear_acceleration":[0,-9.81,0],"linear_drag_coefficient":0.01},
			"minecraft:particle_motion_collision":{"collision_drag":-4,"collision_radius":0.2},
			"minecraft:particle_appearance_billboard":{
				"size":[0.2,0.2],"facing_camera_mode":"lookat_xyz",
				"uv":{"texture_width":128,"texture_height":128,"flipbook":{
					"base_UV":[56,88],"size_UV":[8,8],"step_UV":[-8,0],"frame_per_second":8,"max_frame":8,"stretch_to_lifetime":true
				}}
			},
			"minecraft:particle_appearance_tinting":{"color":[0,1,0]},
			"minecraft:particle_lifetime_expression":{"max_lifetime":"4+v.particle_random_1*2"}
		}
	}
}

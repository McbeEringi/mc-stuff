{
	"format_version":"1.10.0",
	"particle_effect":{
		"description":{
			"identifier":"toko:chunk",
			"basic_render_parameters":{"material":"particles_blend","texture":"textures/particle/particles"}
		},
		"components":{
			"minecraft:emitter_initialization":{"per_update_expression":"v.t=q.time_stamp/20;v.m=math.mod(v.t,2)<1?1:-1;v.t=v.t*8;v.x=math.clamp(math.abs(math.mod(v.t,8)-4)-2,-1,1)*7.9375;v.y=v.m*8;v.z=math.clamp(math.abs(math.mod(v.t+2,8)-4)-2,-1,1)*7.9375;v.dx=(math.abs(v.x)<7.9375)*-math.sign(v.z);v.dz=(math.abs(v.z)<7.9375)*math.sign(v.x);v.x=v.x-8+math.mod(math.mod(q.position(0),16)+16,16);v.y=-v.y+8-math.mod(math.mod(q.position(1)-1.8,16)+16,16)-1.8;v.z=v.z-8+math.mod(math.mod(q.position(2),16)+16,16);v.s=math.sin(-q.body_y_rotation);v.c=math.cos(-q.body_y_rotation);"},
			"minecraft:emitter_rate_steady":{"spawn_rate":20,"max_particles":640},
			"minecraft:emitter_lifetime_looping":{"active_time":2},
			"minecraft:emitter_shape_point":{"offset":["v.x*v.c-v.z*v.s","v.y","v.x*v.s+v.z*v.c"],"direction":["v.dx*v.c-v.dz*v.s","20*v.m","v.dx*v.s+v.dz*v.c"]},

			"minecraft:particle_initial_spin":{"rotation":"math.atan(-20)*v.m","rotation_rate":"30*(v.particle_random_3*2-1)"},
			"minecraft:particle_initial_speed":2.4,
			"minecraft:particle_motion_dynamic":{},
			"minecraft:particle_appearance_billboard":{
				"size":[1.6,1.6],"facing_camera_mode":"direction_x",
				"uv":{"texture_width":128,"texture_height":128,"flipbook":{
					"base_UV":[56,"v.particle_random_2<.5?0:88"],"size_UV":[8,8],"step_UV":[-8,0],"frame_per_second":8,"max_frame":8,"stretch_to_lifetime":true
				}}
			},
			"minecraft:particle_appearance_tinting":{"color":[1,1,1,0.2]},
			"minecraft:particle_lifetime_expression":{"max_lifetime":"v.t<0?0:(12+v.particle_random_1*8)"}
		}
	}
}
